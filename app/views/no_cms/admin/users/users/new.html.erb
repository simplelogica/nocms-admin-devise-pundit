<% content_for :second_menu do %>
  <%= render partial: 'user_listing_item', collection: @users, as: 'user', layout: 'user_listing' unless @users.blank? %>
<% end %>


<%= no_cms_admin_form_for @user do |f| %>
  <%= render partial: 'no_cms/admin/shared/toolbar', locals: { f: f } %>
  <div class="content">
    <%= render 'no_cms/admin/shared/log_messages' %>
    <%= render 'content_fields', f: f %>
  </div>
<% end %>
